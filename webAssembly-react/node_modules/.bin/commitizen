#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules/commitizen/bin/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules/commitizen/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules/commitizen/bin/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules/commitizen/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/commitizen@4.3.0_@types+node@18.11.18_typescript@5.6.3/node_modules:/Users/kevinzzzzzz/Documents/code/webAssembly_learn/webAssembly-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitizen/bin/commitizen" "$@"
else
  exec node  "$basedir/../commitizen/bin/commitizen" "$@"
fi
